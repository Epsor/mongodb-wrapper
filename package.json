{
  "name": "@epsor/mongodb-wrapper",
  "version": "2.0.0",
  "description": "Mongodb wrapper",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "babel src -d dist --ignore 'src/**/*.test.js'",
    "test": "NODE_ENV=test jest --coverage",
